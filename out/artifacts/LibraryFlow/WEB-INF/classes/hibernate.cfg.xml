<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
    "-//Hibernate/Hibernate Configuration DTD//EN"
    "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.show_sql">true</property>

      <mapping resource="springlibrary/Album.hbm.xml"/>
      <mapping resource="springlibrary/Author.hbm.xml"/>
      <mapping resource="springlibrary/Genre.hbm.xml"/>
      <mapping resource="springlibrary/PublishDate.hbm.xml"/>
      <mapping resource="springlibrary/Song.hbm.xml"/>
      <mapping resource="springlibrary/Vote.hbm.xml"/>

  </session-factory>
</hibernate-configuration>